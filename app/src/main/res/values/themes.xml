<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CriticalRoll" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/dull_dark_purple</item>
        <item name="colorPrimaryVariant">@color/maroon</item>
        <item name="colorOnPrimary">@color/pastel_purple</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/dull_dark_purple</item>
        <item name="colorSecondaryVariant">@color/maroon</item>
        <item name="colorOnSecondary">@color/pastel_purple</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="LaunchTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@mipmap/tigerlillylogo</item>
        <!--<item name="android:windowBackground">@drawable/droid20</item> -->
    </style>
</resources>